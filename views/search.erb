<form method="post" action="/search">
<div class="form-group row">
  <div class="col-sm-12">
    <input type="text" name="search" class="form-text form-control-lg"/>
    <input type="submit" value="Search" class="btn btn-primary" />
  </div>
</div>
</form>

<h4>Founded <%= @data.count %></h3>

<% @data.each do |row| %>
  <%= erb(:founded, layout: false, locals: { row: row }) %>
<% end %>